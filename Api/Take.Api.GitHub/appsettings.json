{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Settings": {
    "HealthChecksUiUrl": "http://localhost:80",
    "BlipBotSettings": {
      "Identifier": "botIdentifier",
      "AccessKey": "accessKey",
      "Authorization": "authorization"
    },
    "CheckAuthorizationKey": true,
    "ApiGitHubSetting": {
      "Url": "#{GitHub-GitHubUrl}#"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "#{GitHub-SeqUrl}#",
          "apiKey": "#{GitHub-SeqKey}#"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
